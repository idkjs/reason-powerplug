{
  "name": "reason-powerplug",
  "version": "0.1.2",
  "description": "A set of reuseable `components` and `functors` for  reason react",
  "homepage": "https://github.com/beizhedenglong/reason-powerplug",
  "repository": {
    "type": "git",
    "url": "https://github.com/beizhedenglong/reason-powerplug.git"
  },
  "bugs": {
    "url": "https://github.com/beizhedenglong/reason-powerplug/issues"
  },
  "scripts": {
    "format": "yarn format:most && yarn format:re && yarn format:res",
    "format:most": "prettier --write \"**/*.{bs.js,js,json,css,md}\"",
    "format:re": "find . -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "format:res": "rescript format -all",
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack watch",
    "serve": "webpack serve"
  },
  "keywords": [
    "BuckleScript",
    "Reason React",
    "Rescript React",
    "ReasonML",
    "components",
    "dumb",
    "state",
    "RenderProps"
  ],
  "author": {
    "name": "Victor",
    "url": "https://github.com/beizhedenglong",
    "email": "wang_shaojie@icloud.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "rescript": "^9.1.4",
    "@rescript/react": "^0.10.3"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.7.0",
    "rescript": "^9.1.4",
    "bs-react-test-renderer": "^3.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@rescript/react": "^0.10.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {},
  "prettier": {
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "lint-staged": {
    "*.re": [
      "bsrefmt --in-place",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
